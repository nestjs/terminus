{
  "name": "@nestjs/terminus",
  "version": "8.0.6",
  "description": "Nest - modern, fast, powerful node.js web framework (@terminus)",
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs/terminus"
  },
  "license": "MIT",
  "author": "Livio Brunner",
  "main": "dist/index.js",
  "scripts": {
    "build": "gulp build && gulp move:protos",
    "build:all": "gulp build:all",
    "build:samples": "gulp install:samples && npm run build && gulp build:samples && gulp test:samples && gulp test:e2e:samples",
    "clean": "gulp clean",
    "format": "prettier --write \"{lib,e2e}/**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "npm run test:ut",
    "test:e2e": "jest --config=e2e/jest-e2e.json --detectOpenHandles --forceExit",
    "test:ut": "jest --config=jest.json --detectOpenHandles",
    "test:ut:cov": "npm run test:ut -- --coverage",
    "release": "release-it"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "check-disk-space": "3.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-angular": "16.2.3",
    "@grpc/grpc-js": "1.6.6",
    "@grpc/proto-loader": "0.6.9",
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@nestjs/microservices": "8.4.4",
    "@nestjs/mongoose": "9.0.0",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/platform-fastify": "8.4.4",
    "@nestjs/sequelize": "8.0.0",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/testing": "8.4.4",
    "@nestjs/typeorm": "8.0.3",
    "@release-it/conventional-changelog": "4.3.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/mongoose": "5.11.97",
    "@types/node": "16.11.27",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "amqp-connection-manager": "4.1.2",
    "amqplib": "0.8.0",
    "cli-color": "2.0.2",
    "delete-empty": "3.0.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "fastify": "3.28.0",
    "fastify-formbody": "5.2.0",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-debug": "4.0.0",
    "gulp-typescript": "5.0.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.8",
    "mongoose": "5.13.14",
    "mysql2": "2.3.3",
    "nats": "2.6.1",
    "portfinder": "1.0.28",
    "prettier": "2.6.2",
    "redis": "4.0.6",
    "reflect-metadata": "0.1.13",
    "release-it": "14.14.2",
    "rxjs-compat": "6.6.7",
    "sequelize": "6.19.0",
    "sequelize-typescript": "2.1.3",
    "sort-package-json": "1.55.0",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typeorm": "0.3.6",
    "typescript": "4.6.3",
    "wait-port": "0.2.9"
  },
  "peerDependencies": {
    "@nestjs/common": "8.x",
    "@nestjs/core": "8.x",
    "reflect-metadata": "0.1.x",
    "rxjs": "7.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}
