{
  "name": "@nestjs/terminus",
  "version": "9.0.0",
  "description": "Terminus integration provides readiness/liveness health checks for NestJS.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs/terminus"
  },
  "license": "MIT",
  "author": "Livio Brunner",
  "main": "dist/index.js",
  "scripts": {
    "build": "gulp build && gulp move:protos",
    "build:all": "gulp build:all",
    "build:samples": "gulp install:samples && npm run build && gulp build:samples && gulp test:samples && gulp test:e2e:samples",
    "clean": "gulp clean",
    "format": "prettier --write \"{lib,e2e,sample,tools}/**/*.ts\"",
    "format:check": "prettier --check \"{lib,e2e,sample,tools}/**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "npm run test:ut",
    "test:e2e": "jest --config=e2e/jest-e2e.json --detectOpenHandles --forceExit",
    "test:ut": "jest --config=jest.json --detectOpenHandles",
    "test:ut:cov": "npm run test:ut -- --coverage",
    "release": "release-it",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "check-disk-space": "3.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-angular": "17.0.3",
    "@grpc/grpc-js": "1.6.7",
    "@grpc/proto-loader": "0.6.13",
    "@mikro-orm/core": "5.2.3",
    "@mikro-orm/mongodb": "5.2.3",
    "@mikro-orm/mysql": "5.2.3",
    "@mikro-orm/nestjs": "5.1.0",
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "9.0.3",
    "@nestjs/core": "9.0.3",
    "@nestjs/microservices": "9.0.3",
    "@nestjs/mongoose": "9.2.0",
    "@nestjs/platform-express": "9.0.3",
    "@nestjs/platform-fastify": "9.0.3",
    "@nestjs/sequelize": "9.0.0",
    "@nestjs/swagger": "6.0.4",
    "@nestjs/testing": "9.0.3",
    "@nestjs/typeorm": "9.0.0",
    "@release-it/conventional-changelog": "5.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.5",
    "@types/node": "16.11.30",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "amqp-connection-manager": "4.1.3",
    "amqplib": "0.10.0",
    "cli-color": "2.0.3",
    "delete-empty": "3.0.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "fastify": "4.2.1",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-debug": "4.0.0",
    "gulp-typescript": "5.0.1",
    "husky": "8.0.1",
    "ioredis": "5.2.0",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "mongoose": "6.4.4",
    "mysql2": "2.3.3",
    "nats": "2.7.1",
    "portfinder": "1.0.28",
    "prettier": "2.7.1",
    "reflect-metadata": "0.1.13",
    "release-it": "15.1.1",
    "rxjs-compat": "6.6.7",
    "sequelize": "6.21.3",
    "sequelize-typescript": "2.1.3",
    "sort-package-json": "1.57.0",
    "supertest": "6.2.4",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typeorm": "0.3.7",
    "typescript": "4.7.4",
    "wait-port": "0.3.0"
  },
  "peerDependencies": {
    "@nestjs/common": "9.x",
    "@nestjs/core": "9.x",
    "reflect-metadata": "0.1.x",
    "rxjs": "7.x"
  }
}
