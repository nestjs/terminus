{
  "name": "@nestjs/terminus",
  "version": "6.5.6",
  "description": "Nest - modern, fast, powerful node.js web framework (@terminus)",
  "repository": {
    "url": "https://github.com/nestjs/terminus"
  },
  "license": "MIT",
  "author": "Livio Brunner",
  "main": "dist/index.js",
  "scripts": {
    "build": "gulp",
    "build:all": "gulp build:all",
    "clean": "gulp clean",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "npm run test:ut",
    "test:e2e": "jest --config=e2e/jest-e2e.json --detectOpenHandles --forceExit",
    "test:ut": "jest --config=jest.json",
    "test:ut:cov": "npm run test:ut -- --coverage"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json ./package.json",
      "git add"
    ]
  },
  "dependencies": {
    "check-disk-space": "2.1.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@godaddy/terminus": "4.3.1",
    "@nestjs/common": "6.11.8",
    "@nestjs/core": "6.11.8",
    "@nestjs/testing": "6.11.8",
    "@types/jest": "25.1.3",
    "@types/mongoose": "5.7.1",
    "@types/node": "12.12.21",
    "fastify": "2.12.0",
    "fastify-formbody": "3.1.0",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-typescript": "5.0.1",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lint-staged": "10.0.8",
    "mysql": "2.18.1",
    "portfinder": "1.0.25",
    "prettier": "1.19.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",
    "sort-package-json": "1.40.0",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.2"
  },
  "peerDependencies": {
    "@godaddy/terminus": "^4.2.0",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0"
  },
  "optionalDependencies": {
    "@grpc/proto-loader": "0.5.3",
    "@nestjs/microservices": "6.11.8",
    "@nestjs/mongoose": "6.3.1",
    "@nestjs/platform-express": "6.11.8",
    "@nestjs/platform-fastify": "6.11.8",
    "@nestjs/typeorm": "6.3.1",
    "amqp-connection-manager": "3.2.0",
    "amqplib": "0.5.5",
    "grpc": "1.24.2",
    "mongoose": "5.9.2",
    "mqtt": "3.0.0",
    "nats": "1.4.0",
    "redis": "3.0.2",
    "typeorm": "0.2.22"
  }
}
