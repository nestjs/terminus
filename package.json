{
  "name": "@nestjs/terminus",
  "version": "7.1.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@terminus)",
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs/terminus"
  },
  "license": "MIT",
  "author": "Livio Brunner",
  "main": "dist/index.js",
  "scripts": {
    "build": "gulp",
    "build:all": "gulp build:all",
    "clean": "gulp clean",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "npm run test:ut",
    "test:e2e": "jest --config=e2e/jest-e2e.json --detectOpenHandles --forceExit",
    "test:ut": "jest --config=jest.json --detectOpenHandles",
    "test:ut:cov": "npm run test:ut -- --coverage",
    "release": "release-it"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "check-disk-space": "2.1.0",
    "deprecate": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@compodoc/compodoc": "1.1.11",
    "@godaddy/terminus": "4.6.0",
    "@grpc/proto-loader": "0.5.6",
    "@nestjs/common": "7.5.4",
    "@nestjs/core": "7.5.4",
    "@nestjs/microservices": "7.5.4",
    "@nestjs/mongoose": "7.0.2",
    "@nestjs/platform-express": "7.5.4",
    "@nestjs/platform-fastify": "7.5.4",
    "@nestjs/sequelize": "0.2.0",
    "@nestjs/swagger": "4.7.13",
    "@nestjs/testing": "7.5.4",
    "@nestjs/typeorm": "7.1.5",
    "@release-it/conventional-changelog": "2.0.0",
    "@types/deprecate": "1.1.0",
    "@types/express": "4.17.9",
    "@types/jest": "26.0.20",
    "@types/mongoose": "5.7.36",
    "@types/node": "14.14.31",
    "@types/sequelize": "4.28.9",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "amqp-connection-manager": "3.2.2",
    "amqplib": "0.6.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.0.0",
    "eslint-plugin-import": "2.22.1",
    "fastify": "3.11.0",
    "fastify-formbody": "5.0.0",
    "grpc": "1.24.5",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-debug": "4.0.0",
    "gulp-typescript": "5.0.1",
    "husky": "5.0.9",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "mongoose": "5.10.9",
    "mysql": "2.18.1",
    "mysql2": "2.2.5",
    "nats": "1.4.12",
    "portfinder": "1.0.28",
    "prettier": "2.2.1",
    "redis": "3.0.2",
    "reflect-metadata": "0.1.13",
    "release-it": "14.4.1",
    "rxjs": "6.6.3",
    "rxjs-compat": "6.6.3",
    "sequelize": "5.22.3",
    "sequelize-typescript": "1.1.0",
    "sort-package-json": "1.49.0",
    "supertest": "6.1.3",
    "ts-jest": "26.5.1",
    "ts-node": "9.1.1",
    "typeorm": "0.2.31",
    "typescript": "4.0.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}
