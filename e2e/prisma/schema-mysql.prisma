generator client {
  provider = "prisma-client-js"
  output   = "./generated/mysql"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:root@0.0.0.0/test"
}

model Test {
  id Int @id @default(autoincrement())
}
