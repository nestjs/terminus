{
  "name": "004-grpc-app",
  "version": "1.0.0",
  "description": "GRPC ping check app",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "e2e": "jest --config=e2e/jest-e2e.json --forceExit",
    "e2e:watch": "jest --watch --config=e2e/jest-e2e.json",
    "start": "concurrently -n client,server \"npm:start:client\" \"npm:start:server\" --prefix-colors blue,yellow",
    "start:client": "ts-node src/client/main.ts",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "start:server": "ts-node src/server/main.ts",
    "test": "jest",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "test:watch": "jest --watch --config=jest.json"
  },
  "dependencies": {
    "@grpc/proto-loader": "0.5.6",
    "@nestjs/common": "7.6.13",
    "@nestjs/core": "7.6.13",
    "@nestjs/microservices": "7.6.13",
    "@nestjs/platform-express": "7.6.13",
    "@nestjs/terminus": "7.1.1",
    "@nestjs/websockets": "7.6.13",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "fastify": "3.13.0",
    "grpc": "1.24.5",
    "lodash": "4.17.21",
    "redis": "3.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.6"
  },
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "concurrently": "6.0.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  }
}
