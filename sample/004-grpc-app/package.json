{
  "name": "004-grpc-app",
  "version": "1.0.0",
  "description": "GRPC ping check app",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "e2e": "jest --config=e2e/jest-e2e.json --forceExit",
    "e2e:watch": "jest --watch --config=e2e/jest-e2e.json",
    "start": "concurrently -n client,server \"npm:start:client\" \"npm:start:server\" --prefix-colors blue,yellow",
    "start:client": "ts-node src/client/main.ts",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "start:server": "ts-node src/server/main.ts",
    "test": "jest",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "test:watch": "jest --watch --config=jest.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.9.3",
    "@grpc/proto-loader": "0.7.9",
    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "@nestjs/microservices": "9.4.3",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/terminus": "9.2.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "lodash": "4.17.21",
    "redis": "4.6.8",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.5",
    "@types/node": "16.18.54",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "concurrently": "8.2.1",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "jest": "29.7.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
