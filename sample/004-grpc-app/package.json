{
  "name": "004-grpc-app",
  "version": "1.0.0",
  "description": "GRPC ping check app",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "e2e": "jest --config=e2e/jest-e2e.json --forceExit",
    "e2e:watch": "jest --watch --config=e2e/jest-e2e.json",
    "start": "concurrently -n client,server \"npm:start:client\" \"npm:start:server\" --prefix-colors blue,yellow",
    "start:client": "ts-node src/client/main.ts",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "start:server": "ts-node src/server/main.ts",
    "test": "jest",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "test:watch": "jest --watch --config=jest.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.3.7",
    "@grpc/proto-loader": "0.6.5",
    "@nestjs/common": "8.0.9",
    "@nestjs/core": "8.0.9",
    "@nestjs/microservices": "8.0.9",
    "@nestjs/platform-express": "8.0.9",
    "@nestjs/terminus": "8.0.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "lodash": "4.17.21",
    "redis": "3.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.0"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/node": "14.17.28",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "concurrently": "6.3.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "jest": "27.3.1",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-node": "10.3.1",
    "typescript": "4.4.4"
  }
}
