{
  "name": "004-grpc-app",
  "version": "1.0.0",
  "description": "GRPC ping check app",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "e2e": "jest --config=e2e/jest-e2e.json --forceExit",
    "e2e:watch": "jest --watch --config=e2e/jest-e2e.json",
    "start": "concurrently -n client,server \"npm:start:client\" \"npm:start:server\" --prefix-colors blue,yellow",
    "start:client": "ts-node src/client/main.ts",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "start:server": "ts-node src/server/main.ts",
    "test": "jest --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "test:watch": "jest --watch --config=jest.json"
  },
  "dependencies": {
    "@grpc/proto-loader": "0.5.5",
    "@nestjs/common": "7.5.4",
    "@nestjs/core": "7.5.4",
    "@nestjs/microservices": "7.5.4",
    "@nestjs/platform-express": "7.5.4",
    "@nestjs/websockets": "7.5.4",
    "@nestjs/terminus": "7.0.1",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "fastify": "3.7.0",
    "grpc": "1.24.3",
    "lodash": "4.17.20",
    "redis": "3.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "typescript": "4.0.3"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.10",
    "concurrently": "5.3.0",
    "jest": "26.6.3",
    "supertest": "6.0.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "tslint": "6.1.3"
  }
}
