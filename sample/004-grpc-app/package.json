{
  "name": "004-grpc-app",
  "version": "1.0.0",
  "description": "GRPC ping check app",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "e2e:watch": "jest --watch --config=e2e/jest-e2e.json",
    "start": "concurrently -n client,server \"npm:start:client\" \"npm:start:server\" --prefix-colors blue,yellow",
    "start:client": "ts-node src/client/main.ts",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "start:server": "ts-node src/server/main.ts",
    "test": "jest",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "e2e": "echo 'No e2e tests implemented yet.'",
    "test:watch": "jest --watch --config=jest.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "catalog:",
    "@grpc/proto-loader": "catalog:",
    "@nestjs/terminus": "link:../../",
    "@nestjs/common": "catalog:",
    "@nestjs/core": "catalog:",
    "@nestjs/microservices": "catalog:",
    "@nestjs/platform-express": "catalog:",
    "class-transformer": "catalog:",
    "class-validator": "catalog:",
    "lodash": "catalog:",
    "redis": "catalog:",
    "reflect-metadata": "catalog:",
    "rxjs": "catalog:"
  },
  "devDependencies": {
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "concurrently": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-import": "catalog:",
    "jest": "catalog:",
    "supertest": "catalog:",
    "ts-jest": "catalog:",
    "ts-node": "catalog:",
    "typescript": "catalog:"
  }
}
