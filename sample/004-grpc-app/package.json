{
  "name": "004-grpc-app",
  "version": "1.0.0",
  "description": "GRPC ping check app",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "e2e": "jest --config=e2e/jest-e2e.json --forceExit",
    "e2e:watch": "jest --watch --config=e2e/jest-e2e.json",
    "start": "concurrently -n client,server \"npm:start:client\" \"npm:start:server\" --prefix-colors blue,yellow",
    "start:client": "ts-node src/client/main.ts",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "start:server": "ts-node src/server/main.ts",
    "test": "jest",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "test:watch": "jest --watch --config=jest.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.9.14",
    "@grpc/proto-loader": "0.7.10",
    "@nestjs/common": "10.3.0",
    "@nestjs/core": "10.3.0",
    "@nestjs/microservices": "10.3.0",
    "@nestjs/platform-express": "10.3.0",
    "@nestjs/terminus": "10.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "lodash": "4.17.21",
    "redis": "4.6.12",
    "reflect-metadata": "0.1.14",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.11",
    "@types/node": "20.10.5",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "concurrently": "8.2.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
