{
  "name": "004-grpc-app",
  "version": "1.0.0",
  "description": "GRPC ping check app",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "e2e:watch": "jest --watch --config=e2e/jest-e2e.json",
    "start": "concurrently -n client,server \"npm:start:client\" \"npm:start:server\" --prefix-colors blue,yellow",
    "start:client": "ts-node src/client/main.ts",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "start:server": "ts-node src/server/main.ts",
    "test": "jest",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "e2e": "echo 'No e2e tests implemented yet.'",
    "test:watch": "jest --watch --config=jest.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.12.6",
    "@grpc/proto-loader": "0.7.13",
    "@nestjs/common": "11.0.9",
    "@nestjs/core": "11.0.9",
    "@nestjs/microservices": "11.0.9",
    "@nestjs/platform-express": "11.0.9",
    "@nestjs/terminus": "11.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "lodash": "4.17.21",
    "redis": "4.7.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "22.13.10",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "concurrently": "9.1.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "jest": "29.7.0",
    "supertest": "7.0.0",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  }
}
